generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma" // or "../generated/prisma/client" if you prefer deeper nesting
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  username  String @unique
  password  String
}